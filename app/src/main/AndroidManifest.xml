<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fitwebappclient">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitWebAppClient"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".usermain.ui.mycourses.openmycourse.OpenMyCourseActivity"
            android:exported="false" />
        <activity
            android:name=".adminmain.ui.allcourses.managecourse.ManageCourseActivity"
            android:exported="false" />
        <activity
            android:name=".adminmain.ui.createuser.AdminActivityCreateUser"
            android:exported="false" />
        <activity
            android:name=".adminmain.MainAdminActivity"
            android:exported="false"
            android:label="@string/title_activity_main_user"
            android:theme="@style/Theme.FitWebAppClient.NoActionBar" />
        <activity
            android:name=".usermain.MainUserActivity"
            android:exported="false"
            android:label="@string/title_activity_main_user"
            android:theme="@style/Theme.FitWebAppClient.NoActionBar" />
        <activity
            android:name=".loginandregistration.MainLoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>